<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>RestrictionService</display-name>
  <servlet>
    <servlet-name>RestrictionServlet</servlet-name>
    <servlet-class>anton.RestrictionService</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TradeComplianceHelpist</servlet-name>
    <servlet-class>TradeCompliance.jsp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TradeComplianceHelpist</servlet-name>
    <url-pattern>/TradeComplianceHelpist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RestrictionServlet</servlet-name>
    <url-pattern>/TradeComplianceResultist</url-pattern>
  </servlet-mapping>

</web-app>
